			<Алгоритм Имя="ШаблонПервичногоДокумента" ИспользуетсяПриЗагрузке="false">
				<Текст>СтруктураПД = Новый Структура("Дата, Номер, Организация, Контрагент, Договор, Валюта, СуммаДокумента, СуммаРегл, ДатаВходящегоДокумента, НомерВходящегоДокумента, Подразделение, Комментарий, ТипПервичногоДокумента");
СтруктураПД.Вставить("СуммаУпр", 0);
СтруктураПД.Вставить("ВалютаВзаиморасчетов");
СтруктураПД.Вставить("СуммаВзаиморасчетов", 0);</Текст>
				<Параметры>СтруктураПД</Параметры>
			</Алгоритм>
		</Группа>
		<Группа Имя="Загрузка">
			<Алгоритм Имя="ВыполнитьПроведениеДокументов" ИспользуетсяПриЗагрузке="true">
				<Текст>// Удаление повторяющихся элементов массива
ТекущийИндекс = 0; 
ВсегоЭлементов = Параметры.ДокументыДляПроведения.Количество(); 
Пока ТекущийИндекс &lt; ВсегоЭлементов Цикл 
    Индекс2 = ТекущийИндекс + 1; 
    Пока Индекс2 &lt; ВсегоЭлементов Цикл 
        Если Параметры.ДокументыДляПроведения[Индекс2] = Параметры.ДокументыДляПроведения[ТекущийИндекс] Тогда 
            Параметры.ДокументыДляПроведения.Удалить(Индекс2); 
            ВсегоЭлементов = ВсегоЭлементов - 1; 
        Иначе 
            Индекс2 = Индекс2 + 1; 
        КонецЕсли; 
    КонецЦикла; 
    ТекущийИндекс = ТекущийИндекс + 1; 
КонецЦикла; 

МассивТиповВводОстатков = Новый Массив;
МассивТиповВводОстатков.Добавить(Тип("ДокументОбъект.ВводОстатковВзаиморасчетов"));
МассивТиповВводОстатков.Добавить(Тип("ДокументОбъект.ВводПервоначальныхСведенийДляРасчетаТранспортногоНалога"));
МассивТиповВводОстатков.Добавить(Тип("ДокументОбъект.ВводОстатковВнеоборотныхАктивов2_4"));

Для Каждого Документ Из Параметры.ДокументыДляПроведения Цикл
    Если Не Документ.ЭтоНовый() Тогда
		
		// Переполучаем объект, т.к. он мог быть изменен
		ДокументОбъект = Документ.Ссылка.ПолучитьОбъект();
		Если МассивТиповВводОстатков.Найти(ТипЗнч(ДокументОбъект)) = Неопределено Тогда
            УстановитьОбменДаннымиЗагрузка(ДокументОбъект, Истина);
        КонецЕсли;
	
		ДокументОбъект.ДополнительныеСвойства.Вставить("ОтключитьМеханизмРегистрацииОбъектов", Истина);
		ДокументОбъект.ДополнительныеСвойства.Вставить("НеПроверятьДатыЗапретаИзмененияДанных");
		
		ДокументПроведен = Ложь;
		Если ДокументОбъект.ПроверитьЗаполнение() Тогда
			Попытка
			
				// выполняем попытку проведения документа
				ДокументОбъект.Записать(РежимЗаписиДокумента.Проведение);
				МодульРеглУчетПроведениеСервер = ОбщегоНазначения.ОбщийМодуль("РеглУчетПроведениеСервер");
				Если НЕ МодульРеглУчетПроведениеСервер = Неопределено Тогда
					
					СсылкаДокумента = ДокументОбъект.Ссылка;
					
					СтруктураРеквизиты = Новый Структура(
					"Ссылка, Дата, Организация",
					СсылкаДокумента,
					СсылкаДокумента.Дата,
					СсылкаДокумента.Организация);
					
					МодульРеглУчетПроведениеСервер.ОтразитьДокумент(СтруктураРеквизиты);
				КонецЕсли;
				
				ДокументПроведен = Истина;
				
			Исключение

				ДокументПроведен = Ложь;
				
			КонецПопытки;
		КонецЕсли;
		
		Если НЕ ДокументПроведен Тогда
			Попытка
				ДокументОбъект.Записать(РежимЗаписиДокумента.Запись);
			Исключение       
				
			КонецПопытки;
			
		КонецЕсли;
				
	КонецЕсли;
КонецЦикла;</Текст>
			</Алгоритм>
			<Алгоритм Имя="ЗагрузитьКурсыВалютДляОбъектовРасчета" ИспользуетсяПриЗагрузке="true">
				<Текст>Если РегламентныеЗаданияСлужебный.РегламентноеЗаданиеДоступноПоФункциональнымОпциям(Метаданные.РегламентныеЗадания.ЗагрузкаКурсовВалют) Тогда

	Запрос = Новый Запрос;
	Запрос.УстановитьПараметр("ВалютаРеглУчета", Константы.ВалютаРегламентированногоУчета.Получить());
	Запрос.Текст =
		"ВЫБРАТЬ
		|	ВводОстатковРасчетыСПартнерами.ВалютаВзаиморасчетов КАК Валюта,
		|	ВводОстатковРасчетыСПартнерами.ВалютаВзаиморасчетов.Код КАК КодВалюты,
		|	МИНИМУМ(ПервичныйДокумент.Дата) КАК Дата
		|ИЗ
		|	Документ.ВводОстатковВзаиморасчетов.РасчетыСПартнерами КАК ВводОстатковРасчетыСПартнерами
		|		ПОЛНОЕ СОЕДИНЕНИЕ Документ.ПервичныйДокумент КАК ПервичныйДокумент
		|		ПО ВводОстатковРасчетыСПартнерами.ДокументРасчетов = ПервичныйДокумент.Ссылка
		|ГДЕ
		|	НЕ ВводОстатковРасчетыСПартнерами.ВалютаВзаиморасчетов = &amp;ВалютаРеглУчета
		|	И НЕ ВводОстатковРасчетыСПартнерами.ВалютаВзаиморасчетов = ЗНАЧЕНИЕ(Справочник.Валюты.ПустаяСсылка)
		|
		|СГРУППИРОВАТЬ ПО
		|	ВводОстатковРасчетыСПартнерами.ВалютаВзаиморасчетов";

	ТаблицаВалют = Новый ТаблицаЗначений;
	ТаблицаВалют.Колонки.Добавить("Валюта");
	ТаблицаВалют.Колонки.Добавить("КодВалюты");

	Выборка = Запрос.Выполнить().Выбрать();
	Пока Выборка.Следующий() Цикл
		
		ТаблицаВалют.Очистить();
		СтрокаВалют = ТаблицаВалют.Добавить();
		ЗаполнитьЗначенияСвойств(СтрокаВалют, Выборка);
		
		ПараметрыЗагрузки = Новый Структура;
		ПараметрыЗагрузки.Вставить("НачалоПериода", НачалоДня(Выборка.Дата));
		ПараметрыЗагрузки.Вставить("КонецПериода", Параметры.ДатаОстатков);
		ПараметрыЗагрузки.Вставить("СписокВалют", ТаблицаВалют);
		
		Если Метаданные.Обработки.Найти("ЗагрузкаКурсовВалют") &lt;&gt; Неопределено Тогда
			Обработки["ЗагрузкаКурсовВалют"].ЗагрузитьАктуальныйКурс(ПараметрыЗагрузки);
		КонецЕсли;
	КонецЦикла;
	
КонецЕсли;</Текст>
			</Алгоритм>
			<Алгоритм Имя="НастроитьСпособыОтраженияВРегламентированномУчете" ИспользуетсяПриЗагрузке="true">
				<Текст>// Настроим способы отражения расчетов с партнерами.
// Загруженные ГФУ с указанием счета учета расчетов
СтруктураИзмерений = ОбщийМодуль("НастройкаСчетовУчетаКлиентСервер").ИнициализироватьСтруктуруИзмеренийРегистра();
СтруктураСчетов    = Новый Структура;
Отказ              = Ложь;

Запрос = Новый Запрос("
	|	ВЫБРАТЬ РАЗЛИЧНЫЕ
	|	Ссылка,
	|	Наименование КАК НаименованиеГФУ
	|ИЗ
	|	Справочник.ГруппыФинансовогоУчетаРасчетов
	|");

Выборка = Запрос.Выполнить().Выбрать();
Пока Выборка.Следующий() Цикл
	
	ДлинаНаименования = СтрДлина(Выборка.НаименованиеГФУ);
	НомерПозиции = Найти(Выборка.НаименованиеГФУ, "(");

	СчетУчета = ПланыСчетов.Хозрасчетный.ПустаяСсылка();
	
	Если НомерПозиции &gt; 0 Тогда
		КодСчета = Прав(Выборка.НаименованиеГФУ, ДлинаНаименования - НомерПозиции);
		КодСчета = СтрЗаменить(КодСчета, ")", "");
		КодСчета = СтрЗаменить(КодСчета, "(", "");
		СчетУчета = ПланыСчетов.Хозрасчетный.НайтиПоКоду(КодСчета);
        
	КонецЕсли;	
    Если НЕ ЗначениеЗаполнено(СчетУчета) Тогда
        Продолжить;
    КонецЕсли;
    СтруктураИзмерений.АналитикаУчета = Выборка.Ссылка;
    СтруктураСчетов.Очистить();
